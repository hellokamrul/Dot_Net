
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Education</title>
</head>
<body>
    <div> 
        <h1>Education </h1>
        <table>
            <tr>
                <td>SSC RESULT:  @ViewBag.RESULT</td>
            </tr>
        </table>

        <ul>
            @{ 
                int length = 5;
                int j = 0;
                for (int i = 0; i < length; i++)
                {
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  j = i + 1;
                        <li>@j Hello</li>
                }
            }
         </ul>
    </div>
</body>
</html>
