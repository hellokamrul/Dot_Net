

@{
    ViewBag.Title = "Shop";
}

<h2>Shop</h2>

<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Price</th>
        <th>Qulity</th>
        <th></th>
        <th></th>
    </tr>
    @foreach (var b in Model)
    {
        <tr>
            <td>@b.Id</td>
            <td>@b.Name</td>
            <td>@b.Price</td>
            <td>@b.Qty</td>
            
            <td><a href="" class="btn btn-info">Edit</a></td>
            <td><a href="" class="btn btn-danger">Delete</a></td>
        </tr>
    }
</table>

